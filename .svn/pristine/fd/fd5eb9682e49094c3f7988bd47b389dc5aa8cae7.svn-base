body {
    width: 100%;
    height: 100%;
    background: #fafafa;
}


/*父容器*/

.container {
    width: 100%;
    height: 100%;
    font-size: .875rem;
    /*flex的兼容布局*/
    display: -webkit-box;
    display: -moz-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
    flex-direction: column;
}

/*等级折扣*/
table{
    background: #fff;
    width: 100%;
    height: auto;
    margin-bottom: 1rem;
}
/*表格头类型*/
.th1{
    background: #0CCBF9;
}
.th2{
    background: #FF8A01;
}
.th3{
    background: #EF5D5D;
}
/*表头*/
table thead{
    width: 100%;
}
table thead tr{
    width: 100%;
    height: 2.5rem;
    line-height: 2.5rem;
    text-align: center;
}
table thead tr th{
    width: 33.33%;
    color: #fff;
}
/*表身*/
table tbody{
    width: 100%;
}
table tbody tr{
    width: 100%;
    height: 2rem;
    line-height: 2rem;
    text-align: center;
    border-bottom: 1px solid #ddd;
}
table tbody tr td{
    width: 33.33%;
}
table tbody td{
    border-right: 1px dashed #ddd;
}
table tbody td:last-child{
    border: none;
}

/*积分说明*/
#rankMsg{
    background: #fff;
    padding: 1rem;
    /*flex的兼容布局*/
    display: -webkit-box;
    display: -moz-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
    flex-direction: column;
    justify-content: space-around;
}
#rankMsg p,#rankMsg h3{
    margin-bottom: 1rem;
}
#rankMsg p:last-child{
    margin-bottom: 0;
}