;$(function () {
	var memTab = $('.memTab'),
		recTab = $('.recTab'),
		memTable = $('#memTable'),
		recTable = $('#recTable');
	// tab切换
	memTab.on('click', function() {
		$(this).addClass('true').siblings('div').removeClass('true');
		memTable.show();
		recTable.hide();
	});
	recTab.on('click', function() {
		$(this).addClass('true').siblings('div').removeClass('true');
		recTable.show();
		memTable.hide();
	});
})