html,
body,
div,
p,
ul,
li,
dl,
dt,
dd,
h1,
h2,
h3,
h4,
h5,
h6,
form,
input,
select,
button,
textarea,
iframe,
table,
th,
td {
	margin: 0;
	padding: 0;
}
img {
	border: 0 none;
}
*{list-style: none;} 

html{
	width: 100%;
	height: 100%;
}
body{
	width: 100%;
	height: 100%;
}

/*main start*/
.main{
	width:100%; 
	height:100%; 
	background-color:#efeef4; 
}
.tupian img{
	width:100%;
	height:auto;
	margin:auto;
}
.tupian h3{
	/*line-height:30px;*/
	text-align:center;
	color:#FF0309;
	font-size:.95rem;
	font-family:"微软雅黑";
}
.liebiao{
	width:80%;
	height:auto;
	margin:0 auto;
}
.liebiao ul li {
	width:100%;
	height:50px;
	margin:auto;
	position:relative;
	top:10px;
	
}
.liebiao ul li p{
	width:120px;
	font-family:"微软雅黑";
	font-size:1rem;
	color:#2BA245;
	float:right;
	Line-height:56px;
	text-align:right;
	position:absolute;
}
.liebiao ul li input{
	width:130px;
	float:right;
	position:absolute;
	margin-left:130px;
	top:16px;
	border:none;
	height:25px;
	font-family:"微软雅黑";
	font-size:1rem;
	color:#535353;
	background-color:#efeef4;
}
/*.liebiao ul li span{
	color:red;
}*/
.liebiao ul li select{
	width:100px;
	height:30px;
	position:absolute;
	margin-left:130px;
	top:15px;
	border:solid #c2c1c7 1px;
	height:30px;
	font-family:"微软雅黑";
	font-size:1rem;
	color:#535353;
	background-color:#efeef4;
}
.tijiao{
	width:30%;
	height:2rem;
	margin:0 auto;
}
.tijiao button{ 
	background-color:#2ba245;
	border:none;
	border-radius: 10px;
	width:30%;
	height:2rem;
	text-align:center;
	line-height:2rem;
	font-family:"微软雅黑";
	font-size:1.2rem;
	color:#fefeff;
	position:absolute;
	top:79%;

}