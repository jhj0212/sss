body {
    width: 100%;
    height: 100%;
    background: #fff;
}


/*父容器*/

.container {
    width: 100%;
    height: 100%;
    font-size: .875rem;
    /*flex的兼容布局*/
    display: -webkit-box;
    display: -moz-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
    flex-direction: column;
}
/*头部*/
#imgContainer{
    width: 100%;
    height: 4rem;
    background: url(../images/rechangeBg.png);
    background-size: cover;
}
#form{
    width: 90%;
    height: 2rem;
    margin:1rem auto 0;
    border-radius: .2rem;
    background: red;
    position: relative;
}
#form input{
    width: 95%;
    height: 2rem;
    border-radius: .2rem;
    padding-left: 5%;
    border: none;
    font-size: .875rem;
    outline: none;
}
#form button{
    position: absolute;
    width: 2rem;
    height: 2rem;
    line-height: 0rem;
    border: none;
    border-left: 1px solid #ddd;
    border-radius: 0 .2rem .2rem 0;
    right: 0;
    top: 0;
    background: #fff;
    outline: none;
}
#form button img{
    width: 1rem;
    height: 1rem;
}
/*表格*/
#table{
    width: 100%;
    height: auto;
    background: #fff;
}
/*表格头类型*/
.th1{
    background: #0CCBF9;
}
.th2{
    background: #FF8A01;
}
.th3{
    background: #EF5D5D;
}
.th4{
    background: #BD28A6;
}
/*表头*/
#table thead{
    width: 100%;
}
#table thead tr{
    width: 100%;
    height: 3rem;
    line-height: 3rem;
    text-align: center;
    color: #fff;
}
#table thead th{
    width: 20%;
}
/*表身*/
#table tbody{
    width: 100%;
}
#table tbody tr{
    width: 100%;
    height: 3rem;
    text-align: center;
    border-bottom: 1px solid #ddd;
}
#table tbody td{
    width: 20%;
    border-right: 1px dashed #ddd;
}
#table tbody td:last-child{
    border: none;
}